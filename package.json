{
    "name": "slate-tables",
    "description": "A Slate plugin to handle tables.",
    "version": "0.9.0",
    "license": "Apache-2.0",
    "repository": "git://github.com/George-A-Payne/slate-tables.git",
    "main": "./dist/index.js",
    "types": "./index.d.ts",
    "prettier": "./prettier",
    "scripts": {
        "build": "babel lib --out-dir ./dist --extensions \".tsx,.ts\" --ignore \"**/*.test.ts,**/*.test.tsx,**/*.d.ts\"",
        "start": "start-storybook",
        "build-storybook": "build-storybook -c .storybook -o .out",
        "test": "jest",
        "lint": "eslint ./lib/**/*"
    },
    "jest": {
        "testMatch": [
            "**/?(*.)+(test).[jt]s?(x)"
        ]
    },
    "peerDependencies": {
        "immutable": ">=3.8.1",
        "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
        "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0",
        "slate": "0.44.x",
        "slate-html-serializer": ">=0.7.20"
    },
    "devDependencies": {
        "@babel/cli": "7.8.4",
        "@babel/core": "7.9.0",
        "@babel/plugin-proposal-class-properties": "7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "7.9.0",
        "@babel/preset-env": "7.9.0",
        "@babel/preset-react": "7.9.4",
        "@babel/preset-typescript": "7.9.0",
        "@storybook/addon-docs": "5.3.17",
        "@storybook/addons": "5.3.17",
        "@storybook/react": "5.3.17",
        "@types/jest": "25.1.4",
        "@types/react": "16.9.25",
        "@types/react-dom": "16.9.5",
        "@types/slate-html-serializer": "0.6.3",
        "@types/slate-react": "0.20.3",
        "@typescript-eslint/eslint-plugin": "2.25.0",
        "@typescript-eslint/parser": "2.25.0",
        "babel-jest": "25.1.0",
        "babel-loader": "8.1.0",
        "babel-plugin-module-resolver": "4.0.0",
        "eslint": "6.8.0",
        "eslint-plugin-react": "7.19.0",
        "immutable": "3.8.1",
        "jest": "25.1.0",
        "prettier": "2.0.2",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-is": "16.13.1",
        "slate": "0.44.12",
        "slate-html-serializer": "0.7.36",
        "slate-hyperscript": "0.11.27",
        "slate-react": "0.21.18",
        "typescript": "3.8.3"
    },
    "keywords": [
        "slate"
    ]
}
